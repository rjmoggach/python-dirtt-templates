<?xml version="1.0"?>
<preset version="6">
    <type>sequence</type>
    <comment>For the &quot;{{sequence_name}}&quot; ONLY.
Conformed media is assembled as need be. blah blah.</comment>
    <sequence>
        <fileType>NONE</fileType>
        <namePattern/>
        <includeVideo>True</includeVideo>
        <exportVideo>True</exportVideo>
        <videoMedia>
            <mediaFileType>image</mediaFileType>
            <commit>Original</commit>
            <flatten>NoChange</flatten>
            <exportHandles>True</exportHandles>
            <nbHandles>15</nbHandles>
        </videoMedia>
        <includeAudio>True</includeAudio>
        <exportAudio>False</exportAudio>
        <audioMedia>
            <mediaFileType>audio</mediaFileType>
            <commit>Original</commit>
            <flatten>NoChange</flatten>
            <exportHandles>True</exportHandles>
            <nbHandles>10</nbHandles>
        </audioMedia>
    </sequence>
    <video>
        <fileType>Dpx</fileType>
        <codec>923680</codec>
        <codecProfile/>
        <namePattern>media/work/{{sequence_name}}/&lt;shot name&gt;/plate/transcode/&lt;shot name&gt;_&lt;track name&gt;_&lt;version name&gt;/&lt;shot name&gt;_&lt;track name&gt;_&lt;version name&gt;</namePattern>
        <compressionQuality>50</compressionQuality>
        <transferCharacteristic>2</transferCharacteristic>
        <colorimetricSpecification>4</colorimetricSpecification>
        <publishLinked>True</publishLinked>
        <overwriteWithVersions>False</overwriteWithVersions>
        <resize>
            <resizeType>fit</resizeType>
            <resizeFilter>lanczos</resizeFilter>
            <width>0</width>
            <height>0</height>
            <bitsPerChannel>10</bitsPerChannel>
            <numChannels>3</numChannels>
            <floatingPoint>False</floatingPoint>
            <bigEndian>True</bigEndian>
            <pixelRatio>1</pixelRatio>
            <scanFormat>P</scanFormat>
        </resize>
    </video>
    <name>
        <framePadding>8</framePadding>
        <startFrame>0</startFrame>
        <useTimecode>True</useTimecode>
    </name>
    <createOpenClip>
        <namePattern>clip/work/{{sequence_name}}/plate/transcode/&lt;shot name&gt;_&lt;track name&gt;_v001.</namePattern>
        <version>
            <index>1</index>
            <padding>3</padding>
            <name>v&lt;version&gt;</name>
        </version>
        <batchSetup>
            <namePattern>flame/batch/flame/{{sequence_name}}/&lt;shot name&gt;/&lt;shot name&gt;_grade_&lt;version name&gt;</namePattern>
            <exportNamePattern>clip/work/{{sequence_name}}/plate/grade/&lt;shot name&gt;_grade.</exportNamePattern>
            <outputPathPattern>media/work/{{sequence_name}}/&lt;shot name&gt;/plate/grade/&lt;shot name&gt;_grade_&lt;version name&gt;/&lt;shot name&gt;_grade_&lt;version name&gt;.</outputPathPattern>
            <batchGroup>True</batchGroup>
            <keepClipNodes>True</keepClipNodes>
            <batchGroupPath>/Desktop</batchGroupPath>
        </batchSetup>
    </createOpenClip>
    <reImport>
        <namePattern/>
    </reImport>
</preset>